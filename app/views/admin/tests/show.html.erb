<div class="table-responsive">
  <h3 class="mt-2 mb-2 text-center"><%= t('.list_questions', test:@test.title)%></h3>
  <table class="table table-bordered">
    <thead>
      <tr>
        <th><%= t('.id')%></th>
        <th><%= t('.question')%></th>
        <th colspan="3"><%= t('.action')%></th>
      </tr>
    </thead>
    <tbody>
      <%= render @test.questions %>
    </tbody>
  </table>
</div>
<div class="mt-2">
 <%= link_to t('common.back'), admin_tests_path, class: "btn btn-primary" %>
  <%= link_to t('.add_question'), new_admin_test_question_path(@test), class: "btn btn-primary" %>
</div>
