<div class="container">
  <h3 class="mt-2 mb-2 text-center"><%= t('.header')%></h3>
  <table class="table table-bordered">
    <thead>
      <tr>
        <th><%= t('.id')%></th>
        <th class="sort-by-title pointer">
          <%= t('.name')%>
          <%= octicon 'arrow-up', class: 'text-success hide' %>
          <%= octicon 'arrow-down', class: 'text-success hide' %>
        </th>
        <th><%= t('.level')%></th>
        <th><%= t('.count_question')%></th>
        <th><%= t('.start')%></th>
      </tr>
    </thead>
    <%= render @tests %>
  </table>
</div>
