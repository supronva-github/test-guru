<tr>
  <td><%= test.id %></td>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td><%= link_to test.questions.count, test_questions_path(test.id) %></td>
  <td><%= link_to 'Edit', edit_admin_test_path(test) %></td>
  <td><%= link_to 'Show', admin_test_path(test) %></td>
  <td><%= link_to 'Delete', admin_test_path(test), method: :delete, data: { confirm: 'Are you sure?' } %></td>
</tr>
